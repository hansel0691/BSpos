{
  "env": {
    "browser": true,
  },
  "globals": {
    "angular": false,
  },
  "rules": {
    "camelcase": 2,
    "curly": 2,
    "brace-style": [2, "1tbs"],
    "quotes": [2, "single"],
    "semi": [2, "always"],
    "semi-spacing": [2, {"before": false, "after": true}],
    "comma-spacing": [2, {"before": false, "after": true}],
    "space-in-brackets": [2, "never"],
    "space-infix-ops": 2,
    "no-console": 0
  }
}
